
public class ExemploArray3 {

	public static void main(String[] args) {
		
		String[][] tabuleiro = 
		{
			{"X", "O", "X"},
			{".", "O", "."},
			{"O", "X", "."}
		};

		for (int linha=0; linha < tabuleiro.length; linha++) {
			System.out.print(tabuleiro[linha][0]+" ");
			System.out.print(tabuleiro[linha][1]+" ");
			System.out.print(tabuleiro[linha][2]+" ");
			System.out.println("");
		}

	}

}
